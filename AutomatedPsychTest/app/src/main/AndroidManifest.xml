<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.automatedpsychtest">

    <uses-feature android:name="android.hardware.camera"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:name=".App">
        <activity android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".IsRecordAvailableActivity"
            android:screenOrientation="landscape">

        </activity>

        <activity android:name=".HasRecordActivity"
            android:screenOrientation="landscape">

        </activity>

        <activity android:name=".HasNoRecordActivity"
            android:screenOrientation="landscape">

        </activity>

        <activity android:name=".ConfirmStudentActivity"
            android:screenOrientation="landscape">

        </activity>

        <activity android:name=".ChecklistActivity"
            android:screenOrientation="landscape">

        </activity>

        <activity android:name=".CheckProjectiveActivity"
            android:screenOrientation="landscape">

        </activity>

        <activity android:name=".RetakePhoto"
            android:screenOrientation="landscape">

        </activity>

        <activity android:name=".CheckStudentsActivity"
            android:screenOrientation="landscape">

        </activity>
    </application>
</manifest>
